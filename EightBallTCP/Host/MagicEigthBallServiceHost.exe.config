<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.6" />
    </startup>
  <system.serviceModel>
    <services>
      <service name="MagicEigthBallServiceLib.MagicEigthBallService">
               
        <endpoint address="" 
                  binding="netTcpBinding" 
                  contract="MagicEigthBallServiceLib.IEigthBall"/>
       
        <host>
          <baseAddresses>
            <add baseAddress="net.tcp://localhost:8090/MagicEigthBallService"/>
          </baseAddresses>
        </host>
      </service>
    </services>
	</system.serviceModel>
</configuration>