<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.6" />
    </startup>
  <system.serviceModel>
    <services>
      <service name="MagicEigthBallServiceLib.MagicEigthBallService">
              
        <host>
          <baseAddresses>
            <add baseAddress="http://localhost:8080/MagicEigthBallService"/>
            <add baseAddress="net.tcp://localhost:8099/MagicEithBallService"/>
          </baseAddresses>
        </host>
      </service>
    </services>
    <bindings>
      <basicHttpBinding>
        <binding openTimeout="00:00:30"/>
      </basicHttpBinding>
      <netTcpBinding>
        <binding closeTimeout="00:00:15"/>
      </netTcpBinding>
    </bindings>
    <behaviors>
      <serviceBehaviors>
        <behavior>
          <serviceMetadata httpGetEnabled="true"/>
        </behavior>
      </serviceBehaviors> 
    </behaviors>
    
  </system.serviceModel>
</configuration>